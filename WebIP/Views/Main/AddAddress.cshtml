
@{
    ViewBag.Title = "EditAddress";
}

<h2>EditAddress</h2>

<form method="POST" action="/Main/AddAddress/">

    <input type="hidden" name="Address" value="@ViewBag.Add" />
    <input type="hidden" name="Network" value="@ViewBag.Net" />
    <label>Введіть клієнта</label>
    <input type="text" name="Name"/></br>
    <label>Введіть додаткову інформацію</label>
    <input type="text" name="Information"/></br>
    <input type="submit" value="Отправить" />
</form>

